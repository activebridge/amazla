{
  "configVersion": "v3",
  "app": {
    "appId": 1038805,
    "appName": "Amazla",
    "appType": "app",
    "version": {
      "code": 301,
      "name": "3.0.1"
    },
    "icon": "icon.png",
    "vender": "activebridge",
    "description": "Control your tesla from your watch"
  },
  "permissions": [
    "data:os.device.info",
    "device:os.local_storage",
    "device:os.ble"
  ],
  "runtime": {
    "apiVersion": {
      "compatible": "3.0.0",
      "target": "3.0.0",
      "minVersion": "3.0"
    }
  },
  "targets": {
    "common": {
      "module": {
        "page": {
          "pages": [
            "page/index",
            "page/crypto/index"
          ]
        },
        "app-side": { "path": "app-side/index" },
        "setting": { "path": "setting/index" },
        "secondary-widget": {
          "widgets": [
            {
              "path": "secondary-widget/index",
              "icon": "icon.png",
              "name": "Tesla",
              "preview": "preview",
              "runtime": {
                "type": "js"
              }
            }
          ]
        }
      },
      "platforms": [{ "st": "r" }, { "st": "s" }, { "st": "b" }],
      "designWidth": 480
    }
  },
  "i18n": {
    "en-US": {
      "appName": "Amazla"
    }
  },
  "defaultLanguage": "en-US"
}
